/* To learn more about this file see: https://angular.io/config/tsconfig. */
{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "outDir": "./out-tsc/app",
    "types": [],
    "baseUrl": "src",
    "paths": {
      "@components/*": [
        "app/components/*"
      ],
      "@containers/*": [
        "app/containers/*"
      ],
      "@enums/*": [
        "app/enums/*"
      ],
      "@interfaces/*": [
        "app/interfaces/*"
      ],
      "@models/*": [
        "app/models/*"
      ],
      "@services/*": [
        "app/modules/services/*"
      ],
      "@images/*": [
        "assets/images/*"
      ],
      "@variables/*": [
        "assets/styles/variables/*"
      ],
      "@fonts/*": [
        "assets/fonts/*"
      ],
      "@environments/*": [
        "environments/*"
      ]
    }
  },
  "files": [
    "src/main.ts",
    "src/polyfills.ts"
  ],
  "include": [
    "src/**/*.d.ts"
  ]
}
